#include <stdio.h>
#include "3-calc.h"
/**
 * get_op_func - gets right operator
 * @s: type of operator
 *
 * Return: return int
 */
int (*get_op_func(char *s))(int, int)
{
	op_t ops[] = {
        {"+", op_add},
        {"-", op_sub},
        {"*", op_mul},
        {"/", op_div},
        {"%", op_mod},
        {NULL, NULL}
    };
    int i = 0;

    while (i != NULL)
    {
	    s = strcmp(ops[i]);
    }
    return (s);
}
