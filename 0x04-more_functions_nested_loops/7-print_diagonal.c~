#include "main.h"
/**
 * print_diagonal - entry point
 *
 * @n:variable
 *
 * Return: void
 */
void print_diagonal(int n)
{
int space, slash;
if (n > 1)
  for (slash = 0; slash < n; slash++)
    {
      for (space = 0; space < slash; space++)
	{
	_putchar(' ');
	
	}	
	_putchar('\\');
	_putchar('\n');
}
{
_putchar('\n');
}
}
